{
  "name": "lantern-api",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run migrate && bun run generate && bun run --watch src/index.ts",
    "migrate": "bun run src/scripts/runMigrations.ts --latest",
    "migrate:up": "bun run src/scripts/runMigrations.ts --up",
    "migrate:down": "bun run src/scripts/runMigrations.ts --down",
    "migrate:new": "bun run src/scripts/newMigration.ts",
    "generate": "kysely-codegen --out-file ./gen/database-types.ts --camel-case"
  },
  "dependencies": {
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "elysia": "latest",
    "kysely": "^0.27.4",
    "kysely-bun-worker": "^0.7.0",
    "better-sqlite3": "^11.5.0"
  },
  "devDependencies": {
    "kysely-codegen": "^0.17.0",
    "bun-types": "latest"
  },
  "module": "src/index.js"
}